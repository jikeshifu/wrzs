<view class="content"><topbar style="visibility:hidden;" vue-id="de5bf0ae-1" title="商品详情" bind:__l="__l"></topbar><topbar style="position:fixed;top:0;left:0;width:100%;z-index:1;" vue-id="de5bf0ae-2" title="商品详情" bind:__l="__l"></topbar><view class="swiper-pics"><view class="swiper-contain"><view class="badge">1/1</view><swiper circular="{{true}}" autoplay="{{true}}" interval="{{5000}}" duration="{{500}}"><swiper-item><image src="{{baseUrl+goodsData.goods_image}}" mode="aspectFill"></image></swiper-item></swiper></view><view class="caption">{{goodsData.goods_name}}</view><view class="flex-t"><view class="left"><text>￥</text>{{goodsData.goods_price+''}}</view><view class="right"><image src="{{baseUrl+'/static/imgs/reduce.png'}}" mode="aspectFill" data-event-opts="{{[['tap',[['reduceGoods',['$event']]]]]}}" bindtap="__e"></image><text>{{count}}</text><image src="{{baseUrl+'/static/imgs/add.png'}}" mode="aspectFill" data-event-opts="{{[['tap',[['addGoods',['$event']]]]]}}" bindtap="__e"></image></view></view><view class="flex-b"><view class="left">{{"已售"+goodsData.goods_sold+"件"}}</view><view class="right">{{"库存"+goodsData.goods_stock}}</view></view><view class="panel">{{goodsData.shipping_remarks?goodsData.shipping_remarks:'预计3天发货'}}</view></view><view class="room-desc"><view class="title">商品说明</view><u-parse vue-id="de5bf0ae-3" content="{{goodsData.goods_about}}" noData=" " bind:__l="__l"></u-parse></view><view class="empty-fixed"></view><view class="fixed-bottom"><view class="left"><view class="h4"><text>￥</text>{{totalMoney+''}}</view><view class="p">{{"共"+count+"件"}}</view></view><view class="right"><button class="btn1" type="primary" data-event-opts="{{[['tap',[['addGwcar',['$event']]]]]}}" bindtap="__e">加入购物车</button><button class="btn2" type="primary" data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" bindtap="__e">立即购买</button></view></view><fixed-icons vue-id="de5bf0ae-4" bind:__l="__l"></fixed-icons></view>