y-mm-dd hh:mm') {\r\n        return year + '-' + month + '-' + day + ' ' + hours + ':' + minutes\r\n      }\r\n      if (type === 'yy-mm-dd hh') {\r\n        return year + '-' + month + '-' + day + ' ' + hours + ':' + '00'\r\n      }\r\n    },\r\n    // 时间戳之间的天数\r\n    $diffDate(startTime, endTime) {\r\n      const begintime = new Date(startTime).getTime()\r\n      const endtime = new Date(endTime).getTime()\r\n      const nTime = endtime - begintime\r\n      const day = Math.floor(nTime / 86400000)\r\n      return day\r\n    }\r\n  }\r\n}"],"mappings":"AAAA,eAAe;EACbA,OAAO,EAAE;IACP;IACAC,cAAc,WAAAA,eAACC,IAAI,EAAEC,IAAI,EAAE;MACzB,IAAIC,OAAO,GAAG,IAAIC,IAAI,CAAC,CAACH,IAAI,CAAC;MAC7B,IAAII,IAAI,GAAGF,OAAO,CAACG,WAAW,CAAC,CAAC;MAChC,IAAIC,KAAK,GAAGJ,OAAO,CAACK,QAAQ,CAAC,CAAC,GAAG,CAAC,GAAG,EAAE,GAAG,GAAG,IAAIL,OAAO,CAACK,QAAQ,CAAC,CAAC,GAAG,CAAC,CAAC,GAAGL,OAAO,CAACK,QAAQ,CAAC,CAAC,GAAG,CAAC;MACjG,IAAIC,GAAG,GAAGN,OAAO,CAACO,OAAO,CAAC,CAAC,GAAG,EAAE,GAAG,GAAG,GAAGP,OAAO,CAACO,OAAO,CAAC,CAAC,GAAGP,OAAO,CAACO,OAAO,CAAC,CAAC;MAC9E,IAAIC,KAAK,GAAGR,OAAO,CAACS,QAAQ,CAAC,CAAC,GAAG,EAAE,GAAG,GAAG,GAAGT,OAAO,CAACS,QAAQ,CAAC,CAAC,GAAGT,OAAO,CAACS,QAAQ,CAAC,CAAC;MACnF,IAAIC,OAAO,GAAGV,OAAO,CAACW,UAAU,CAAC,CAAC,GAAG,EAAE,GAAG,GAAG,GAAGX,OAAO,CAACW,UAAU,CAAC,CAAC,GAAGX,OAAO,CAACW,UAAU,CAAC,CAAC;MAC3F,IAAIC,OAAO,GAAGZ,OAAO,CAACa,UAAU,CAAC,CAAC,GAAG,EAAE,GAAG,GAAG,GAAGb,OAAO,CAACa,UAAU,CAAC,CAAC,GAAGb,OAAO,CAACa,UAAU,CAAC,CAAC;MAC3F,IAAId,IAAI,KAAK,UAAU,EAAE;QACvB,OAAOG,IAAI,GAAG,GAAG,GAAGE,KAAK,GAAG,GAAG,GAAGE,GAAG;MACvC;MACA,IAAIP,IAAI,KAAK,mBAAmB,EAAE;QAChC,OAAOG,IAAI,GAAG,GAAG,GAAGE,KAAK,GAAG,GAAG,GAAGE,GAAG,GAAG,GAAG,GAAGE,KAAK,GAAG,GAAG,GAAGE,OAAO,GAAG,GAAG,GAAGE,OAAO;MACrF;MACA,IAAIb,IAAI,KAAK,gBAAgB,EAAE;QAC7B,OAAOG,IAAI,GAAG,GAAG,GAAGE,KAAK,GAAG,GAAG,GAAGE,GAAG,GAAG,GAAG,GAAGE,KAAK,GAAG,GAAG,GAAGE,OAAO;MACrE;MACA,IAAIX,IAAI,KAAK,aAAa,EAAE;QAC1B,OAAOG,IAAI,GAAG,GAAG,GAAGE,KAAK,GAAG,GAAG,GAAGE,GAAG,GAAG,GAAG,GAAGE,KAAK,GAAG,GAAG,GAAG,IAAI;MAClE;IACF,CAAC;IACD;IACAM,SAAS,W